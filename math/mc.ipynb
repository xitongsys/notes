   "source": [
    "import matplotlib.pyplot as plt\n",
    "import pandas as pd\n",
    "import numpy as np\n",
    "\n",
    "n = 100\n",
    "p = n // 2\n",
    "xs = np.linspace(0,1, 100)\n",
    "ys = np.power(xs, p)*np.power(1-xs, n-p)\n",
    "\n",
    "plt.plot(xs, ys)\n",
    "\n"
   ]
  }
